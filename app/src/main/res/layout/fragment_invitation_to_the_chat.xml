<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/invitation_fragment"
    tools:context=".fragments.InvitationToTheChatFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <ImageView
            android:id="@+id/konsul_tantplyus_1"
            android:layout_width="141dp"
            android:layout_height="133dp"
            android:layout_marginBottom="16dp"
            android:contentDescription="konsul_tantplyus_1"
            android:src="@drawable/robots_face"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.94"
            app:layout_constraintStart_toStartOf="parent" />


        <View
            android:id="@+id/ellipse_1"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:layout_marginTop="19dp"
            android:layout_marginEnd="23dp"
            android:alpha="0.98000001907349"
            android:background="@drawable/ellipse_1_shape"
            app:layout_constraintEnd_toEndOf="@+id/konsul_tantplyus_1"
            app:layout_constraintTop_toTopOf="@+id/konsul_tantplyus_1" />


        <View
            android:id="@+id/container"
            android:layout_width="314dp"
            android:layout_height="261dp"
            android:layout_marginBottom="16dp"
            android:alpha="0.60000002384186"
            android:background="@drawable/container_shape"
            app:layout_constraintBottom_toTopOf="@+id/konsul_tantplyus_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.163"
            app:layout_constraintStart_toStartOf="parent" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="288dp"
            android:layout_height="wrap_content"
            android:text="@string/invit_bots_text"
            android:textColor="@color/black"
            android:textSize="20sp"
            app:layout_constraintBottom_toBottomOf="@+id/container"
            app:layout_constraintEnd_toEndOf="@+id/container"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toStartOf="@+id/container"
            app:layout_constraintTop_toTopOf="@+id/container"
            app:layout_constraintVertical_bias="0.448" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>